<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="br.edu.unis.oo.negocio">
    <class name="Cidade" table="tblCidade">
        <id name="codigoIbge" type="int" column="codigoIbge"/>
        <property name="descricao" type="java.lang.String" column="descricao"/>       
        <property name="uf" type="java.lang.String" column="uf"/>
    </class>
</hibernate-mapping>