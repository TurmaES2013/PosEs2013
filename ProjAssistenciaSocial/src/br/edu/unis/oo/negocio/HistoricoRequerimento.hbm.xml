<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="br.edu.unis.oo.negocio">
    <class name="HistoricoRequerimento" table="tblHistRequerimento">
        <id name="id" type="int" column="id"/>
        <property name="data" type="java.util.Date" column="data"/>
        <property name="historico" type="java.util.String" column="historico"/>       
    </class>
</hibernate-mapping>
